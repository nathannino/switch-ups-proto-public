; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Switch-ups"
run/main_scene="res://overlay/main_overlay.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DeferredLoadingManager="*res://globals/autoloads/DeferredLoading/deferred_loading_manager.tscn"
BgmManager="*res://globals/autoloads/bgm_manager/bgm_manager.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Switch-ups"

[rendering]

environment/defaults/default_clear_color=Color(0, 0, 0, 1)
