SpiritDictionary="*res://globals/autoloads/spirit_dictionary/spirit_dictionary.gd"
OptionsOverlay="*res://overlay/options.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Switch-ups"

[editor_plugins]

enabled=PackedStringArray()

[gui]

theme/default_font_generate_mipmaps=true

[input]

options={
